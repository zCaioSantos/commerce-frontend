{
 "extends": "airbnb-typescript-prettier",
 "rules": {
  "react/react-in-jsx-scope": "off",
  "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }],
  "import/prefer-default-export": "off",
  "react/no-unescaped-entities": "off",
  "prettier/prettier": [
   "error",
   {
    "endOfLine": "crlf"
   }
  ]
 }
}
